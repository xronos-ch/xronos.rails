<div class='nested-fields'>

  <div class="field_master_top">
    <b><%= f.label :reference %></b>
  </div>

  <div class="field_master">
    <%= f.fields_for :references do |reference| %>
      <%= render 'references/reference_fields', :f => reference %>
    <% end %>

    <div class = "field">
      <%= f.label :page %>
      <%= f.text_field :page %>
    </div>

  </div>

  <%= fa_icon "minus" %> <%= link_to_remove_association "remove reference", f %>

  <hr>

</div>
