<%= bootstrap_form_with(model: @c14_lab, local: true, id: "c14LabForm") do |f| %>

  <%= render 'fields', f: f %>

  <%= content_for :buttons do %>
    <%= button_tag "Cancel", type: 'reset', class: 'btn btn-danger', form: "c14LabForm", data: { "bs-dismiss": "modal" } %>
    <%= button_tag "Save", class: 'btn btn-success', form: "c14LabForm" %>
  <% end %>

<% end %>

