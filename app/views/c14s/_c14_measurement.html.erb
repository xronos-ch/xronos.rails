<dl class="row">

  <dt>Age <small>(uncal BP)</small></dt>
  <dd class="font-monospace"><%= c14.bp %></dd>

  <dt>Error <small>(Â±)</small></dt>
  <dd class="font-monospace"><%= c14.std %></dd>

  <dt>Lab</dt>
  <dd><%= c14.c14_lab.present? ? c14.c14_lab.name : na_value %></dd>

  <dt>Method</dt>
  <dd><%= c14.method.present? ? c14.method : na_value %></dd>

  <dt>Sample material</dt>
  <dd><%= c14.sample.material.present? ? c14.sample.material.name : na_value %></dd>

  <dt>Sample taxon</dt>
  <dd>
    <% if c14.sample.taxon.present? %>
      <%= render c14.sample.taxon %>
    <% else %>
      <%= na_value %>
    <% end %>
  </dd>

</dl>
