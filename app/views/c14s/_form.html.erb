<%= bootstrap_form_with(model: @c14, local: true, id: "c14Form") do |f| %>
  <%= render  'fields', f: f %>

  <%= content_for :buttons do %>
    <%= button_tag "Cancel", type: 'reset', class: 'btn btn-danger', form: "c14Form", data: { "bs-dismiss": "modal" } %>
    <%= button_tag "Save", class: 'btn btn-success', form: "c14Form" %>
  <% end %>

<% end %>

