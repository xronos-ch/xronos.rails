<table class="table table-striped table-sm">
  <thead>
    <tr>
      <th scope="col">Sampled Archaeological Object</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Material</th>
      <td><%= arch_object.material.name if measurement.lab.present? %></td>
    </tr>
    <tr>
      <td>Species</th>
      <td><%= arch_object.species.name  if measurement.lab.present? %></td>
    </tr>
    <tr>
      <td>Site Phase</th>
      <td><%= arch_object.site_phase.name if measurement.lab.present?%></td>
    </tr>
  </tbody>
</table>
